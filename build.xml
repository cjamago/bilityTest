                <type type="file"/>
            </fileset>
        </copy>
    </target>

    <!-- ==================================================================== -->
    <!-- Run                                                             -->
    <!-- ==================================================================== -->
    <!-- Runs the file and generates Reportng report for TestNG-->
    <property name="lib" value="${module.newmavenproject.basedir}/lib" />
    <property name="bin" value="${module.newmavenproject.basedir}/bin" />

    <!-- ====== Set the classpath ====  -->
    <path id="classpath">
        <pathelement location="${bin}" />
        <fileset dir="${lib}">
            <include name="*.jar" />
        </fileset>
    </path>


    <target  name="run"
             description="run examples with java"
             depends="build.modules">

        <echo message="testng-run "/>
        <java fork="yes"
              classpathref="classpath"
              classname="org.testng.TestNG">
            <arg value="-d"/>
            <arg value="${newmavenproject.testoutput.dir}"/>
            <arg value="${module.newmavenproject.basedir}/testng.xml"/>
            <jvmarg value="-ea"/>
        </java>
        <echo>Some of these tests should have failed.  Statistics are available at</echo>
        <echo>${newmavenproject.testoutput.dir}\index.html for a TestNG HTML report</echo>
    </target>

    <!-- Compile the java code from ${src} into ${bin} -->

    <!-- Runs the file and generates Reportng report for TestNG-->
    <taskdef name="testng" classname="org.testng.TestNGAntTask" classpathref="classpath" />

    <target name="testng-execution" depends="build.modules">
        <mkdir dir="${newmavenproject.testoutput.dir}" />
        <testng outputdir="${newmavenproject.testoutput.dir}" classpathref="classpath" useDefaultListeners="true">
            <xmlfileset dir="${module.newmavenproject.basedir}" includes="testng.xml" />
        </testng>
    </target>



    <!-- ==================================================================== -->
    <target name="clean.module.newmavenproject" description="cleanup module">
        <delete dir="${bin}" />
        <delete dir="${newmavenproject.output.dir}"/>
        <delete dir="${newmavenproject.testoutput.dir}"/>
    </target>

    <!-- Creating directories -->
    <target name="create" depends="clean.module.newmavenproject">
        <mkdir dir="${bin}" />
        <mkdir dir="${newmavenproject.output.dir}"/>
        <mkdir dir="${newmavenproject.testoutput.dir}" />
    </target>

    <target name="init" depends="create" description="Build initialization">
        <!-- Perform any build initialization in this target -->
    </target>

    <target name="clean" depends="clean.module.newmavenproject" description="cleanup all"/>

    <target name="build.modules" depends="init, clean" description="build all modules"/>

    <target name="all" depends="testng-execution" description="build all"/>
