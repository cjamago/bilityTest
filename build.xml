    <target name="compile.module.newmavenproject"
            depends="compile.module.newmavenproject.production,compile.module.newmavenproject.tests"
            description="Compile module NewMavenProject"/>

    <target name="compile.module.newmavenproject.production"
            description="Compile module NewMavenProject; production classes">
        <mkdir dir="${newmavenproject.output.dir}"/>
        <javac destdir="${newmavenproject.output.dir}" includeantruntime="false" debug="${compiler.debug}"
               nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true"
               executable="${module.jdk.bin.newmavenproject}/javac">
            <compilerarg line="${compiler.args.newmavenproject}"/>
            <bootclasspath refid="newmavenproject.module.bootclasspath"/>
            <classpath refid="newmavenproject.module.production.classpath"/>
            <src refid="newmavenproject.module.sourcepath"/>
            <patternset refid="excluded.from.compilation.newmavenproject"/>
        </javac>

        <copy todir="${newmavenproject.output.dir}">
            <fileset dir="${module.newmavenproject.basedir}/src/main/java">
                <patternset refid="compiler.resources"/>
                <type type="file"/>
            </fileset>
        </copy>
    </target>

    <target name="compile.module.newmavenproject.tests" depends="compile.module.newmavenproject.production"
            description="compile module NewMavenProject; test classes" unless="skip.tests">
        <mkdir dir="${newmavenproject.testoutput.dir}"/>
        <javac destdir="${newmavenproject.testoutput.dir}" includeantruntime="${javac2.instrumentation.includeJavaRuntime}" debug="${compiler.debug}"
               nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true"
               executable="${module.jdk.bin.newmavenproject}/javac">
            <compilerarg line="${compiler.args.newmavenproject}"/>
            <bootclasspath refid="newmavenproject.module.bootclasspath"/>
            <classpath refid="newmavenproject.module.classpath"/>
            <src refid="newmavenproject.module.test.sourcepath"/>
            <patternset refid="excluded.from.compilation.newmavenproject"/>
        </javac>

        <copy todir="${newmavenproject.testoutput.dir}">
